(declare-rel R (Bool Bool Bool))
(declare-rel G ())
(declare-var x0 Bool)
(declare-var x1 Bool)
(declare-var y0 Bool)
(declare-var y1 Bool)
(declare-var x Bool)
(declare-var y Bool)
(rule (R true true true))
(rule (=> (and (R x0 y0 x) (R x1 y1 y))
          (R (xor x0 x1) (xor y0 y1) x)))
(rule (=> (R true false y) G))
(query G)
